{
  "name": "public-sync",
  "version": "1.0.4",
  "description": "A tool to sync files from a private directory to a public directory",
  "main": "./src/sync.js",
  "type": "module",
  "bin": {
    "public-sync": "./src/sync.js"
  },
  "scripts": {
    "sync": "node ./src/sync.js",
    "test": "jest"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "keywords": [
    "sync",
    "file-management"
  ],
  "author": "OllyFN",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "fs-extra": "^10.0.0",
    "prompt": "^1.3.0",
    "read-pkg-up": "^11.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  }
}
